<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="CORS">
    <DisplayName>CORS</DisplayName>
    <FaultRules/>
    <Properties/>
    <Add>
        <Headers>
            <Header name="Access-Control-Allow-Origin">GRUNT_CONFIG</Header>
            <Header name="Access-Control-Allow-Headers">GRUNT_CONFIG</Header>
            <Header name="Access-Control-Max-Age">GRUNT_CONFIG</Header>
            <Header name="Access-Control-Allow-Methods">GRUNT_CONFIG</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
